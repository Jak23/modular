
<end>
    time limit : 1999.0

<capture>
    increment : 1.0 : time

<species>
    t1   : 0
    a1   : 0
    T1   : 0
    A1   : 0
    TT1  : 0
    C1   : 0
    CC1  : 0

    t2   : 0
    a2   : 0
    T2   : 0
    A2   : 0
    TT2  : 0
    C2   : 0
    CC2  : 0

<variables>
    # transcription parameters
    sigma1 : 20.0
    k_01   : 0.05
    sigma2 : 20.0
    k_02   : 0.05

    # translation parameters
    beta1 : 100.0
    nu1    : 1.25
    beta2 : 100.0
    nu2    : 1.25

    # complex formation
    k_C : 10.0

    # cleavage
    f_T1   : 5.0
    omega1 : 0.2
    f_T2   : 5.0
    omega2 : 0.2

    # degradation
    mu1     : 150.0
    mu2     : 150.0
    eta     : 0.5
    epsilon : 0.05

    # dilution
    gamma : 1.0
    b_c1  : 0.125
    b_c2  : 0.125

<functions>
    mod1degtargs : A1 + C1 + CC1
    mod2degtargs : A2 + C2 + CC2

    fback1 : 1 / (1 + mod1degtargs * k_01)
    fback2 : 1 / (1 + mod2degtargs * k_02)
    lambda_ta1 : sigma1 * fback1
    lambda_ta2 : sigma2 * fback2

    alpha_a1 : beta1 * nu1
    alpha_t1 : beta1
    alpha_a2 : beta2 * nu2
    alpha_t2 : beta2
    
    F_a1 : f_T1 * T_total1 * omega1
    F_t1 : f_T1 * T_total1
    F_a2 : f_T2 * T_total2 * omega2
    F_t2 : f_T2 * T_total2

    kappa11 : 1.0-eta
    kappa21 : eta
    kappa12 : eta
    kappa22 : 1.0-eta

    Mu1 : mu1 / (epsilon + kappa11 * mod1degtargs + kappa21 * mod2degtargs)
    Mu2 : mu2 / (epsilon + kappa12 * mod1degtargs + kappa22 * mod2degtargs)
    G1 : kappa11 * Mu1 + kappa12 * Mu2
    G2 : kappa21 * Mu1 + kappa22 * Mu2

    #Gamma : gamma/(1.0 + b_c1 * T_total1 + b_c2 * T_total2)
    Gamma : gamma/(1.0 + b_c1 * T_total1)
    Gamma2 : gamma/(1.0 + b_c2 * T_total2)

    T_total1 : T1 + TT1
    A_total1 : A1
    C_total1 : C1 + CC1

    T_total2 : T2 + TT2
    A_total2 : A2
    C_total2 : C2 + CC2

<reactions>
    1 a1 alpha_a1 -> 1 a1 + 1 A1 : translation of antitoxin mRNA of module1
    1 t1 alpha_t1 -> 1 t1 + 1 T1 : translation of toxin mRNA of module1
    nothing lambda_ta1 -> 1 t1 + 1 a1 : productioof toxin/antitoxin mRNAs of module1

    1 a2 alpha_a2 -> 1 a2 + 1 A2 : translation of antitoxin mRNA of module2
    1 t2 alpha_t2 -> 1 t2 + 1 T2 : translation of toxin mRNA of module2
    nothing lambda_ta2 -> 1 t2 + 1 a2 : productioof toxin/antitoxin mRNAs of module2

    2 T1 k_C -> 1 TT1 : toxin dimer formation of module1
    2 T2 k_C -> 1 TT2 : toxin dimer formation of module2
    1 TT1 + 1 A1  k_C -> 1 C1 : formation of TTA complex of module 1 
    1 TT2 + 1 A2  k_C -> 1 C2 : formation of TTA complex of module 2
    2 C1 k_C -> 1 CC1 : formation of TTAATT complex of module 1
    2 C2 k_C -> 1 CC2 : formation of TTAATT complex of module 2

    1 t1 + F_t1 -> nothing : cleavage of toxin mRNA by toxin of module 1
    1 a1 + F_a1 -> nothing : cleavage of antitoxin mRNA by toxin of module 1
    1 t2 + F_t2 -> nothing : cleavage of toxin mRNA by toxin of module 2
    1 a2 + F_a2 -> nothing : cleavage of antitoxin mRNA by toxin of module 2

    1 A1 G1 -> nothing : degradation of antitoxin1
    1 C1 G1 -> 1 TT1 : degradation of antitoxin in TTA complex1
    1 CC1 G1 -> 1 TT1 + 1 C1 : degradation of antitoxin in TTAATT complex1

    1 A2 G2 -> nothing : degradation of antitoxin1
    1 C2 G2 -> 1 TT2 : degradation of antitoxin in TTA complex1
    1 CC2 G2 -> 1 TT2 + 1 C2 : degradation of antitoxin in TTAATT complex1

    1 a1 Gamma -> nothing : dilution of antitoxin1 mRNA
    1 A1 Gamma -> nothing : dilution of antitoxin1
    1 C1 Gamma -> nothing : dilution of TTA complex1
    1 CC1 Gamma -> nothing : dilution of TTAATT complex1
    1 t1 Gamma -> nothing : dilution of toxin1 mRNA
    1 T1 Gamma -> nothing : dilution of toxin1
    1 TT1 Gamma -> nothing : dilution of toxin1 dimer

    1 a2 Gamma2 -> nothing : dilution of antitoxin2 mRNA
    1 A2 Gamma2 -> nothing : dilution of antitoxin2
    1 C2 Gamma2 -> nothing : dilution of TTA complex2
    1 CC2 Gamma2 -> nothing : dilution of TTAATT complex2
    1 t2 Gamma2 -> nothing : dilution of toxin2 mRNA
    1 T2 Gamma2 -> nothing : dilution of toxin2
    1 TT2 Gamma2 -> nothing : dilution of toxin2 dimer

<targets>
    time
    T_total1
    A_total1
    C_total1
    T_total2
    A_total2
    C_total2

<parameterspace>
	<map> : 100
    omega1 : 0.1,0.5 : 0.25 : 5
    eta : 0.0,0.5 : 0.0 : 2

<measurements>
    bistability : 0;location : T_total1,T_total2 of time
    bypspace : 1;parameterspace : all
    correlation : 0;location : T_total1 and T_total2 of time : 5 : 0.0
    bypspace : 3;parameterspace : all

<outputs>
    none : ensemble_output    : none : all
    none : bistability_output : none : all
    none : bistabreorg_output : pkl  : all
    none : correlation_output : pkl  : all
    none : correlreorg_output : pkl  : all






