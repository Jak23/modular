
<end_criteria>
	time limit : 0.005

<capture_criteria>
  increment : 0.00005 : time

<variables>
  k1 : 1.0
  k2 : 0.01
  mu : 800.0

<reactions>
	1 Enzyme + 1 Substrate k1 -> 1 ES_Complex : Enzyme Substrate complex formation
	1 ES_Complex k2 -> 1 Enzyme + 1 Substrate : Enzyme Substrate complex dissociation
	1 ES_Complex mu -> 1 Enzyme + 1 Product   : Product formation

<species>
	Enzyme : 10
	Substrate : 10
	ES_Complex : 10
	Product : 10

<plot_targets>
	time
	ES_Complex
	Product
	Substrate
	Enzyme

<parameter_space>
	<fitting_space> #
    k1 : value : 1.0
    k2 : value : 0.01
    mu : value : 800.0
    #k1 : value : 0.000001-10000.0;2
    #k2 : value : 0.000001-10000.0;2
    #mu : value : 0.000001-10000.0;2
    Enzyme     : initial : 1-10000000;2
    Substrate  : initial : 1-10000000;2
    ES_Complex : initial : 1-10000000;2
    Product    : initial : 1-10000000;2

<fit_routines>
  #anexplorer : explorer : decimated : mm_fit_input.0.pkl : mm_kinetics_map.mmap
  #anannealer : annealing : decimated : mm_fit_input.0.pkl : mm_kinetics_map.mmap
  #alearner   : learning  : magnitude : mm_fit_input.0.pkl : mm_kinetics_map.mmap

  alearner : learning : magnitude : 10000 : 600 : 10000 : 10000 : 10 : < anannealer % annealing % magnitude % 1000 % 300 % 1000 % 1000 % 1 % mm_fit_input.0.pkl % mm_kinetics.mmap > : mm_kinetics.mmap
  #anannealer : annealing : decimated : 1000 : 300 : 1000 : 1000 : 100 : mm_fit_input.0.pkl : mm_kinetics_map.mmap

<multiprocessing>
	multiprocessing : off
	workers : 16

<clustering>
  distributed : off
  simulations_per_job : 10

<output_plans>
	1 : - : fitroutn_output : pkl : all
	0 : - : ensemble_output : pkl : all

<ensemble>
	mapparameterspace : off
	metamapparameterspace : off
	fitting : on
	postprocessing : off
	trajectory_count : 100

